/dts-v1/;
/plugin/;
&i2c1 {
    i2c-scl-rising-time-ns = <185>;
    i2c-scl-falling-time-ns = <20>;
    clock-frequency = <400000>;
    status = "okay";

    bmp180@77 {
        compatible = "MyI2CSensors, bmp180";
        reg = <0x77>;
        status = "okay"
    };

}
dtc -@ -1 dts -0 dtb -o device_tree.dtbo device_tree.dts
// Enable extra leds in the /sys/class/leds
compatible = "brcm,bcm2835";
/dts-v1/;
/plugin/;
/ {
	

	fragment@0 {
		target = <&i2c1>;
		__overlay__ {
			bmp180: bmp180@77 {
				compatible = "MyI2CSensors,bmp180";
				reg = <0x77>;
                status = "okay"
			};
		};
	};
};
