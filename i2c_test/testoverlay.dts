&i2c4 {
    i2c-scl-rising-time-ns = <185>;
    i2c-scl-falling-time-ns = <20>;
    clock-frequency = <400000>;
    status = "okay";

    bmp180@EF {
        compatible = "MyI2CSensors, bmp180";
        reg = <0xEE>;
        status = "okay"
    };

    bmp180_wr: bmp180_wr@EE {
        compatible = "MyI2CSensors.bmp180";
        reg = <0xEE>;
    }
}

